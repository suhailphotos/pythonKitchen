[tool.poetry]
name = "pythonKitchen"
version = "0.1.6"
description = "Package description for pythonKitchen"
authors = ["Suhail <suhailece@gmail.com>"]
license = "MIT"
readme = "README.md"
packages = [{include = "pythonkitchen", from = "src"}]

[tool.poetry.scripts]
pythonkitchen = "pythonkitchen.cli:main"

[tool.poetry.dependencies]
python = ">=3.11,<4.0"
prefect = {extras = ["github"], version = "^3.1.5"}
incept = "^0.1.78"           # ^ plus your new patch
oauthmanager = "^0.1.1"
tabulate = "^0.9.0"
pandas = "^2.2.3"
requests = "^2.32.3"
browser-cookie3 = "^0.20.1"
openpyxl = "^3.1.5"
beautifulsoup4 = "^4.13.4"
selenium = "^4.31.0"
webdriver-manager = "^4.0.2"
openai = "^1.76.0"
pillow = "^11.2.1"
pytesseract = "^0.3.13"
python-dotenv = "^1.1.0"
pypdf = "^5.5.0"
notion-client = "^2.3.0"
ledu = "^0.1.3"
lumiera = "^0.1.3"
parivaha = "^0.1.16"

[tool.poetry.group.dev.dependencies]
build = "^1.2.2.post1"
twine = "^6.1.0"

[build-system]
requires = ["poetry-core"]
build-backend = "poetry.core.masonry.api"
